{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "trainingdatafactoryv2"
		},
		"BlobMetadataSQLConnection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobMetadataSQLConnection'"
		},
		"OrganizedDLSConnection_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'OrganizedDLSConnection'"
		},
		"OrdersBlobStorageConnection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'OrdersBlobStorageConnection'"
		},
		"TrainingKeyVaultConnection_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://trainingkeyvaultv2.vault.azure.net/"
		},
		"DLSTargetProviderFunctionConnection_properties_typeProperties_functionAppUrl": {
			"type": "string",
			"defaultValue": "https://dlstargetprovider20201125160234.azurewebsites.net"
		},
		"OrganizedDLSConnection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://trainingdlsv2.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "trainingdatafactoryv2_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"OrganizedDLSConnection_accountKey": {
						"value": "[parameters('OrganizedDLSConnection_accountKey')]"
					},
					"OrdersBlobStorageConnection_connectionString": {
						"value": "[parameters('OrdersBlobStorageConnection_connectionString')]"
					},
					"BlobMetadataSQLConnection_connectionString": {
						"value": "[parameters('BlobMetadataSQLConnection_connectionString')]"
					},
					"TrainingKeyVaultConnection_properties_typeProperties_baseUrl": {
						"value": "[parameters('TrainingKeyVaultConnection_properties_typeProperties_baseUrl')]"
					},
					"OrganizedDLSConnection_properties_typeProperties_url": {
						"value": "[parameters('OrganizedDLSConnection_properties_typeProperties_url')]"
					},
					"DLSTargetProviderFunctionConnection_properties_typeProperties_functionAppUrl": {
						"value": "[parameters('DLSTargetProviderFunctionConnection_properties_typeProperties_functionAppUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}